    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.3.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  }
